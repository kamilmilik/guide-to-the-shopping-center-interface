<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="kamilmilik.przewodnikpogaleriihandlowej.MainView.accesandcontact.ContactFragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <RelativeLayout
            android:id="@+id/relativeContact1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="15dp">

            <TextView
            android:id="@+id/textContact1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Adres"
            android:textSize="25dp" />

            <TextView
                android:id="@+id/textContact2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textContact1"
                android:layout_marginTop="6dp"
                android:textSize="18dp"
                android:text="ul. Bobrzynskiego 33" />
            <TextView
                android:id="@+id/textContact3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textContact2"
                android:textSize="18dp"
                android:text="30-102 Krakow" />

            <TextView
                android:id="@+id/textContact4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
