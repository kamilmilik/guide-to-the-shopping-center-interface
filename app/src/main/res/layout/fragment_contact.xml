                android:textSize="25dp" />

            <TextView
                android:id="@+id/textContact14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textContact13"
                android:layout_marginTop="6dp"
                android:textSize="18dp"
                android:text="12 123 123 741" />
            <TextView
                android:id="@+id/textContact15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textContact14"
                android:textSize="18dp"
                android:text="parking@galeriaruczaj.pl" />


        </RelativeLayout>

    </RelativeLayout>
</FrameLayout>
