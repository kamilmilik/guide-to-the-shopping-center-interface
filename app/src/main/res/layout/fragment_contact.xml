                android:layout_marginTop="6dp"
                android:textSize="18dp"
                android:text="12 123 456 789" />
            <TextView
                android:id="@+id/textContact6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textContact5"
                android:textSize="18dp"
                android:text="info@galeriaruczaj.pl" />
            <TextView
                android:id="@+id/textContact7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Dział Marketingu"
                android:layout_below="@+id/textContact6"
                android:layout_marginTop="20dp"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/textContact8"
